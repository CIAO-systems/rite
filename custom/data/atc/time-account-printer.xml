<rite>
    <plugins>
        <plugin id="common" path="target/debug" name="rite_common"/>
        <plugin id="csv" path="target/debug" name="rite_csv"/>
        <plugin id="atc" path="target/debug" name="rite_atc"/>
    </plugins>
    <processes>
        <process id="Time account example">
            <importer plugin="atc" name="dataset">
                <configuration>
                    <config key="url" value="$ATC_URL" />
                    <config key="auth-token" value="rite" />
                    <config key="user" value="$ATC_USER" />
                    <config key="password" value="$ATC_PASSWORD" />
                    <config key="filter.table" value="Account" />
                    <!-- <config key="filter.fields" value="name,value,actualstart" /> -->
                </configuration>
            </importer>
            <exporters>
                <exporter plugin="common" name="console" />
                <exporter plugin="csv">
                    <configuration>
                        <config key="filename" value="/tmp/accounts.csv" />
                    </configuration>
                </exporter>
            </exporters>
        </process>
    </processes>
</rite>