<rite>
    <plugins>
        <plugin id="common" path="target/debug" name="rite_common"/>
        <plugin id="faker" path="target/debug" name="rite_faker"/>
        <plugin id="ciao" path="target/debug" name="rite_ciao"/>
    </plugins>
    <processes>
        <process id="Project exporter with fake data example">
            <importer plugin="faker">
                <configuration xml="$RITE_CONFIG_PATH/fake-projects-generator.xml" />
            </importer>
            <exporters>
                <exporter plugin="common" name="console" />
                <exporter plugin="ciao" name="projects">
                    <configuration>
                        <config key="url" value="$CIAO_URL" />
                        <config key="api-key" value="$CIAO_API_KEY" />
                    </configuration>
                </exporter>
            </exporters>
        </process>
    </processes>
</rite>