<rite>
    <plugins>
        <plugin id="common" path="target/debug" name="rite_common"/>
        <plugin id="ciao" path="target/debug" name="rite_ciao"/>
        <plugin id="postgres" path="target/debug" name="rite_postgres"/>
    </plugins>
    <processes>
        <process id="Project example">
            <importer plugin="ciao" name="projects">
                <configuration>
                    <config key="url" value="$CIAO_URL" />
                    <config key="api-key" value="$CIAO_API_KEY" />
                </configuration>
            </importer>
            <exporters>
                <exporter plugin="common" name="console" />
                <exporter plugin="postgres">
                    <configuration xml="$RITE_CONFIG_PATH/postgres-export-config.xml" />
                </exporter>
            </exporters>
        </process>
    </processes>
</rite>