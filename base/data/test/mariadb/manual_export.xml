<rite-mariadb-export>
    <connection 
        host="localhost"
        port="3306"
        database="mariadb"
        user="root"
        password="${MARIADB_PASSWORD:rootpassword}"
    />
    <table name="test_users" uniqueFields="id">
        <create>
        <![CDATA[
        CREATE TEMPORARY TABLE test_users (
            id INT PRIMARY KEY,
            name VARCHAR(100),
            age INT
        )
        ]]>
        </create>
    </table>
</rite-mariadb-export>