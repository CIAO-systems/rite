<rite>
    <plugins>
        <plugin id="common" path="$RITE_CONFIG_PATH/../../target/debug" name="rite_common"/>
        <plugin id="rest" path="$RITE_CONFIG_PATH/../../target/debug" name="rite_rest"/>
    </plugins>
    <processes>
            <process id="Test listing customers in key manager">
            <importer plugin="rest">
                <configuration>
                    <config key="url" value="$KEY_MANAGER_URL/customers" />
                    <config key="auth.api-key" value="x-api-key:$KEY_MANAGER_API_KEY" />
                </configuration>
            </importer>
            <exporters>
                <exporter plugin="common" name="console">
                    <configuration>
                        <config key="prefix" value="Customer {" />
                        <config key="postfix" value="}" />
                        <config key="separator" value="&#xA;" />
                        <config key="field-prefix" value=" " />
                    </configuration>
                </exporter>
            </exporters>
        </process>

        <process id="Test listing keys in key manager">
            <importer plugin="rest">
                <configuration>
                    <config key="url" value="$KEY_MANAGER_URL/keys" />
                    <config key="auth.api-key" value="x-api-key:$KEY_MANAGER_API_KEY" />
                </configuration>
            </importer>
            <exporters>
                <exporter plugin="common" name="console">
                    <configuration>
                        <config key="prefix" value="Key {" />
                        <config key="postfix" value="}" />
                        <config key="separator" value="&#xA;" />
                        <config key="field-prefix" value=" " />
                    </configuration>
                </exporter>
            </exporters>
        </process>
    </processes>
</rite>