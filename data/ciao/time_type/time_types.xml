<rite>
    <plugins>
        <plugin id="common" path="target/debug" name="rite_common"/>
        <plugin id="ciao" path="target/debug" name="rite_ciao"/>
    </plugins>
    <processes>
        <process id="Time types example">
            <importer plugin="ciao" name="time_types">
                <configuration>
                    <config key="url" value="$CIAO_URL" />
                    <config key="api-key" value="$CIAO_API_KEY" />
                </configuration>
            </importer>
            <exporters>
                <exporter plugin="common" name="console">
                    <configuration>
                        <config key="prefix" value="Time type {" />
                        <config key="postfix" value="}" />
                        <config key="separator" value="&#xA;" />
                        <config key="field-prefix" value=" " />
                    </configuration>
                </exporter>
                <exporter plugin="common" name="template">
                    <configuration>
                        <config key="templateFile" value="$RITE_CONFIG_PATH/mapper-template.xml" />
                        <config key="outputFile" value="$RITE_CONFIG_PATH/time-type-mapping.xml" />
                    </configuration>
                </exporter>
            </exporters>
        </process>
    </processes>
</rite>