<rite-youtrack-import>
    <!-- 
        see
        https://www.jetbrains.com/help/youtrack/devportal/resource-api-workItems.html
    -->
    <dataset 
        path="workItems"
        query="created: {2025-01-01} .. {Today}"
        > 
        <fields>
            <field id="id" />
            <field id="created" />
            <field id="duration(minutes)" />
            <field id="author(email)" />
            <!-- 
            <field id="id" />
            <field id="updated" />
            <field id="created" />
            <field id="author(login,email)" />
            <field id="creator(login,email)" />
            <field id="duration(id,minutes)" />
            <field id="text" />
            <field id="type(id,name,autoAttached)" /> 
            <field id="issue(idReadable,summary)" />
            -->
        </fields>
    </dataset>
</rite-youtrack-import>